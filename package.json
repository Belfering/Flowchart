{
  "name": "atlas-forge",
  "version": "0.1.0",
  "description": "Multi-indicator branch generation and backtesting tool",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && node --watch index.mjs",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "start": "cd server && node index.mjs"
  },
  "keywords": ["trading", "backtesting", "technical-indicators", "yfinance"],
  "author": "Trader",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
